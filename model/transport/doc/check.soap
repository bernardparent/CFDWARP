#!/usr/local/bin/soap

P=1300;
T=1000;
chieminus=1e-4;


kB=1.38e-23;
calA=6.023e23;
N=P/kB/T;
Ne=chieminus*N;
Ni=chieminus*N;
Nn=N-Ne-Ni;
mi=0.032/calA;

muin=0.75E23/Nn/sqrt(T);
muii=14.3*T^1.5/Ni/sqrt(mi);

mui=1/(1/(muin)+1/(muii));


printf("muii=%E\n",muii);
printf("muin=%E\n",muin);
printf("mui=%E\n",mui);
